{
    "name": "Google Drive Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Add a context menu to the image in Google Drive",
  
    "content_scripts": [
      {
        "matches": ["https://drive.google.com/drive/*"],
        "js": [
          "./content.js"
        ]
      }
    ]
  }